<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrap.css")
    @Styles.Render("~/Content/bootstrap.min.css")
        @Styles.Render("~/Content/Site.css")
        @Scripts.Render("~/bundles/modernizr")
        <script type="text/javascript" src="~/Content/datepicker/js/jquery.js"></script>
        <link rel="stylesheet" href="~/Content/datepicker/css/datepicker.css"  type="text/css" />
        <link rel="stylesheet" href="~/Content/datepicker/css/layout.css" media="screen" type="text/css" />
        <script type="text/javascript" src="~/Content/datepicker/js/datepicker.js"></script>
        <script type="text/javascript" src="~/Content/datepicker/js/eye.js"></script>
        <script type="text/javascript" src="~/Content/datepicker/js/layout.js"></script>
        <script type="text/javascript" src="~/Content/datepicker/js/utils.js"></script>
    <style type="text/css">
        

            div.container1 {
                background-color: orange;
                padding: 10px;
            }
    </style>
</head>
<body>
    <div>
        <div class="container1" >
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Syracuse University Tour", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("HomePage", "HomePage", "Site")</li>
                    <li>@Html.ActionLink("Entity Framework", "Index", "Site")</li>
                    <li>@Html.ActionLink("EventCalendar", "Cal", "Site")</li>
                    <li>@Html.ActionLink("GoogleMap", "googleMap", "Site")</li>
                    <li>@Html.ActionLink("Syracuse Resource", "Syracuse", "Site")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    @*<div>*@
        @RenderBody()
        @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@
    @*</div>*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    
</body>
</html>
